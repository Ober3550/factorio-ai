{
  "name": "factorio-ai",
  "type": "module",
  "version": "1.0.0",
  "description": "An exercise in defining the process of composing tools to design factorio blueprints",
  "main": "index.js",
  "scripts": {
    "decode-bp": "node --no-warnings --loader ts-node/esm scripts/decode-blueprint.ts",
    "bp-ascii": "node --no-warnings --loader ts-node/esm scripts/blueprint-ascii.ts",
    "decode-and-ascii": "node --no-warnings --loader ts-node/esm scripts/run-decode-and-ascii.ts",
    "test-ascii": "node --no-warnings --loader ts-node/esm scripts/test-ascii.ts",
    "bp-meta": "node --no-warnings --loader ts-node/esm scripts/blueprint-metadata.ts",
    "bp-flow": "node --no-warnings --loader ts-node/esm scripts/blueprint-flow.ts",
    "compose-bp": "node --no-warnings --loader ts-node/esm scripts/compose-blueprint.ts"
  },
  "bin": {
    "bp-meta": "scripts/blueprint-metadata.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ober3550/factorio-ai.git"
  },
  "keywords": [
    "factorio",
    "ai"
  ],
  "author": "Ober3550",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ober3550/factorio-ai/issues"
  },
  "homepage": "https://github.com/Ober3550/factorio-ai#readme",
  "devDependencies": {
    "typescript": "^5.9.3",
    "ts-node": "^10.9.1",
    "@types/node": "^18.16.19"
  }
}
